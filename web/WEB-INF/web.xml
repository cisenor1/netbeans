<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>MultiServ</servlet-name>
        <servlet-class>servlet.MultiServ</servlet-class>
        <init-param>
            <param-name>username</param-name>
            <param-value>cisenor1</param-value>
        </init-param>
        <init-param>
            <param-name>password</param-name>
            <param-value>Compy123</param-value>
        </init-param>
        <init-param>
            <param-name>title</param-name>
            <param-value>Multiplication Server</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>MultiServ</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
